  "author": "zeloff <zelofflab@yandex.ru>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --open --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "analyze": "webpack --config ./config/webpack.analyze.js",
    "test": "jest --watch"
  },
  "husky": {
    "hooks": {
